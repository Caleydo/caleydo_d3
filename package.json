{
  "name": "caleydo_d3",
  "description": "D3 utilitities for Caleydo Web",
  "version": "0.0.1",
  "license" : "SEE LICENSE IN LICENSE",
  "repository": "Caleydo/caleydo_d3",
  "peerDependencies": {
    "caleydo_core": "*"
  },
  "caleydo": {
    "type": "plugin",
    "category": "utility",
    "plugins": {
      "web": [
        {
          "type": "ajax-adapter",
          "id": "caleydo_adapter_d3",
          "file": "ajax_adapter_d3"
        },
        {
          "type": "autoload",
          "id": "caleydo_links",
          "file": "link_renderer"
        },
        {
          "type": "link-representation",
          "id": "link-block",
          "file": "link_representation",
          "name": "Block Link Representation",
          "factory": "createBlockRep",
          "granularity": 0
        },
        {
          "type": "link-representation",
          "id": "link-group",
          "file": "link_representation",
          "name": "Group Link Representation",
          "factory": "createGroupRep",
          "granularity": 5
        },
        {
          "type": "link-representation",
          "id": "link-item",
          "file": "link_representation",
          "name": "Item Link Representation",
          "factory": "createItemRep",
          "granularity": 10
        }
      ]
    },
    "dependencies": {
      "web": {
        "d3": "~3.5.5"
      },
      "tsd": {
        "d3": "626e4a"
      }
    }
  },
  "publishConfig": {
    "registry": "http://registry.caleydo.org/"
  }
}
